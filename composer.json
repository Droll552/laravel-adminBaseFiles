{
    "name": "geo-sot/laravel-adminbasefiles",
    "description": "A laravel Package that Supports the use of global Settings ",
    "keywords": [
        "geo-sot",
        "laravel",
        "laravel-adminBase"
    ],
    "authors": [
        {
            "name": "Geo Sot",
            "email": "geo.sotis@gmail.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "laravel/ui": "^2.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-translation-manager": "^0.5.8",
        "cviebrock/eloquent-sluggable": "^7",
        "geo-sot/laravel-env-editor": "^0.9.4",
        "kris/laravel-form-builder": "^1.30",
        "lab404/laravel-impersonate": "^1.6",
        "laravel/passport": "^9.3",
        "mcamara/laravel-localization": "^1.5",
        "rap2hpoutre/laravel-log-viewer": "^1.4",
        "santigarcor/laratrust": "^6.1",
        "spatie/laravel-image-optimizer": "^1.6",
        "spatie/laravel-translatable": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "GeoSot\\BaseAdmin\\": "src/"
        },
        "files": [
            "src/Helpers/FlashMessages.php",
            "src/Helpers/Base.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
        },
        "laravel": {
            "providers": [
                "GeoSot\\BaseAdmin\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "laravel/framework": "6.x",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "~3.7"
    },
    "autoload-dev": {
        "psr-4": {
            "GeoSot\\BaseAdmin\\Tests\\": "tests/"
        }
    }
}
